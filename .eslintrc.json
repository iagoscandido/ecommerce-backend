{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "indent": ["error", 2],
        "class-methods-use-this": "off",
        "padded-blocks": "off",
        "nonblock-statement-body-position":"off",
        "curly": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_"}]
    }
}
